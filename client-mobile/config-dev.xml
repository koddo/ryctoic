<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.egotv.ryctoic.client.redirect" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ryctoic</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="redirect.html" />
    <!-- <content src="index_test.html" /> -->


    <!-- TODO: disable this (in js) for ios < 7 -->
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    

    <preference name="orientation" value="all" />
    
    <!-- newer won't work until cordova ios platform 4.0 -->
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <platform name="android">
        <allow-intent href="*" />
    </platform>
    <platform name="ios">
        <allow-intent href="*" />
    </platform>

    
    <preference name="DisallowOverscroll" value="true"/>

    <!-- <preference name="SplashScreenDelay" value="100" /> -->
    <preference name="SplashScreen" value="none"/>


    <preference name="BackupWebStorage" value="none"/>
</widget>
